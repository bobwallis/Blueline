{% from 'BluelineCCCBRDataBundle:Macros:paging.html.twig' import paging %}
<section class="search">
	<ol>
	{% for association in associations %}
		<li><a href="{{ path( 'Blueline_Associations_view', { 'abbreviation': association.abbreviation } ) }}">{{ association.name }}</a></li>
	{% else %}
		<li>No results</li>
	{% endfor %}
	</ol>
	{{ paging( 'Blueline_Associations_search', searchVariables, pageActive, pageCount ) }}
</section>
